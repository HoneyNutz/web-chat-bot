{
  "name": "personal-website-chatbot",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "build:cf": "vite build",
    "preview:cf": "npm run build && npx -y wrangler@3 pages dev .svelte-kit/cloudflare --project-name personal-website-chatbot",
    "deploy:cf": "npm run build && npx -y wrangler@3 pages deploy .svelte-kit/cloudflare --project-name personal-website-chatbot",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "lint": "npm run lint:eslint && prettier --check .",
    "lint:eslint": "eslint . --ext .ts,.svelte",
    "lint:fix": "eslint . --ext .ts,.svelte --fix && prettier --write .",
    "ingest": "tsx scripts/ingest.ts",
    "py:install": "uv pip install -r python/requirements.txt",
    "py:server": "PY_EMBED_HOST=127.0.0.1 PY_EMBED_PORT=8000 uv run python python/embed_server.py",
    "ingest:py": "uv run python python/py_ingest.py",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@sveltejs/kit": "^2.5.10",
    "openai": "^4.55.4",
    "svelte": "^4.2.18"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^4.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte-check": "^3.8.5",
    "svelte-eslint-parser": "^1.3.1",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  }
}
